<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/signimagetwo"
    tools:context=".Resume">

    <ImageView
        android:id="@+id/IV_back_reume"
        android:layout_width="19dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:src="@drawable/ic_back_arrow_black" />
    <TextView
        android:id="@+id/uploadresume"
        android:layout_width="@dimen/_85sdp"
        android:layout_height="@dimen/_35sdp"
        android:text="Upload Resume (PDF)"
        android:gravity="center"
        android:textColor="#000"
        android:layout_marginTop="@dimen/_180sdp"
        android:background="@drawable/orangeborder_whiteback"
        android:layout_marginLeft="@dimen/_105sdp"
        />
    <TextView
        android:id="@+id/viewResume"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginTop="@dimen/_45sdp"
        android:layout_marginRight="@dimen/_80sdp"
        android:text="View Resume"
        android:layout_marginLeft="@dimen/_110sdp"
        android:textColor="#000"/>



</LinearLayout>